{
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["vite/client","element-plus/global"],
    "baseUrl": ".",
    "skipLibCheck": true,
    "paths": {
      "@/*":["src/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "vite.config.ts"
  ],
  "exclude": ["node_modules/**", "dist", "**/*.js"]
}
